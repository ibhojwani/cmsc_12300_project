runners:
  dataproc:
    jobconf:
      mapreduce.task.timeout: 3600000
      initializationActions.executionTimeout: 20m

    bootstrap_python: True

    bootstrap:
    - sudo apt-get update
    - sudo apt-get install python3-pip -y
    - sudo apt-get install python3-numpy 
    - sudo apt-get install python3-scipy -y